    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Attach)",
            "request": "attach",
            "hostName": "localhost",
            "port": 5051
        },
        {
            "type": "java",
            "name": "CodeLens (Launch) - RefreshIG",
            "request": "launch",
            "mainClass": "org.opencds.cqf.igtools.RefreshIG",
            "projectName": "tooling"
        },
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Connectathon FHIR3",
            "request": "launch",
            "mainClass": "org.opencds.cqf.Main",
            "projectName": "tooling",
            "args": [
                "-RefreshIG",
                "-ip=C:\\src\\GitHub\\connectathon\\fhir3",
                "-rp=C:\\src\\GitHub\\connectathon\\fhir3\\resources",
                "-iv=fhir3",
                "-t",
                "-d",
                "-v",
                "-p"
            ]
        },
        {
            "type": "java",
            "name": "Connectathon FHIR4",
            "request": "launch",
            "mainClass": "org.opencds.cqf.Main",
            "projectName": "tooling",
            "args": [
                "-RefreshIG",
                "-ip=C:\\src\\GitHub\\connectathon\\fhir4",
                "-t",
                "-d",
                "-v",
                "-p"
            ]
        }        
    ]
}